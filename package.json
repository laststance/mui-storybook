{
  "name": "mui-storybook",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "storybook build",
    "clean": "rimraf dist node_modules pnpm-lock.yaml",
    "format": "prettier --write \"**/*.+(json|yml|css|md|mdx)\"",
    "gen": "plop",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "sort-package-json": "sort-package-json",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "vitest --project=storybook  --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "validate": "./scripts/validate"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@chromatic-com/storybook": "^4.1.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint/js": "^9.39.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/lab": "7.0.1-beta.20",
    "@mui/material": "^7.3.6",
    "@mui/system": "^7.3.6",
    "@mui/types": "^7.4.9",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.7",
    "@storybook/addon-docs": "^10.1.7",
    "@storybook/addon-mcp": "^0.1.6",
    "@storybook/addon-themes": "^10.1.7",
    "@storybook/addon-vitest": "^10.1.7",
    "@storybook/react-vite": "^10.1.7",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitest/browser": "^4.0.15",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-istanbul": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "all-contributors-cli": "^6.26.1",
    "axe-playwright": "^2.2.2",
    "chromatic": "^13.3.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.1.7",
    "globals": "^16.5.0",
    "http-server": "^14.1.1",
    "playwright": "^1.57.0",
    "plop": "^4.0.4",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rimraf": "^6.1.2",
    "sort-package-json": "^3.5.1",
    "storybook": "^10.1.7",
    "storybook-i18n": "^10.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vitest": "^4.0.15",
    "wait-on": "^9.0.3"
  }
}
